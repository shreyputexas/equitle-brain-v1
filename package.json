{
  "name": "equitle-brain",
  "version": "1.0.0",
  "description": "Enterprise deal management platform with AI-powered knowledge base",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --exec \"ts-node --transpile-only server/src/index.ts\"",
    "dev:client": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:client": "tsc && vite build",
    "build:server": "tsc -p server/tsconfig.json",
    "preview": "vite preview",
    "start": "node dist/server/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "clean": "rm -rf .vite node_modules/.cache server/node_modules/.cache dist server/dist *.tsbuildinfo server/*.tsbuildinfo",
    "fresh-start": "./scripts/dev-reset.sh",
    "dev:reset": "./scripts/dev-reset.sh && npm run dev"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^5.18.0",
    "@mui/material": "^5.18.0",
    "@mui/x-data-grid": "^7.3.1",
    "@mui/x-date-pickers": "^7.3.1",
    "@pinecone-database/pinecone": "^2.2.1",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/ws": "^8.18.1",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.4.5",
    "elevenlabs-node": "^2.0.3",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "express-ws": "^5.0.2",
    "firebase": "^12.3.0",
    "firebase-admin": "^13.5.0",
    "googleapis": "^159.0.0",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.3",
    "langchain": "^0.2.2",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.47.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pdfjs-dist": "^4.2.67",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "recharts": "^2.12.6",
    "redis": "^4.6.13",
    "retell-sdk": "^4.49.0",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.1",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.8",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
