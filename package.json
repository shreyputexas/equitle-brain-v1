{
  "name": "equitle-brain",
  "version": "1.0.0",
  "description": "Enterprise deal management platform with AI-powered knowledge base",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cross-env DOTENV_CONFIG_PATH=.env nodemon --exec \"ts-node --transpile-only -r dotenv/config server/src/index.ts\"",
    "dev:client": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:client": "tsc && vite build",
    "build:server": "tsc -p server/tsconfig.json",
    "build:production": "NODE_ENV=production npm run build:server",
    "start": "node dist/server/index.js",
    "start:production": "NODE_ENV=production node dist/server/index.js",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "postinstall": "npm run build:server"
  },
  "dependencies": {
