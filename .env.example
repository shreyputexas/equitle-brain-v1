# Environment Variables Example
# Copy this file to .env and fill in your actual values

# ElevenLabs API (already configured)
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Slybroadcast Credentials (for mass voicemail delivery)
SLYBROADCAST_EMAIL=your_slybroadcast_email@example.com
SLYBROADCAST_PASSWORD=your_slybroadcast_password

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key-here

# Firebase Configuration
FIREBASE_PROJECT_ID=equitle-brain-dev
FIREBASE_SERVICE_ACCOUNT_PATH=./firebase-service-account.json
FIREBASE_USE_EMULATORS=true

# Server Configuration
NODE_ENV=development
PORT=4001

# API Keys (use dummy values for development)
APOLLO_API_KEY=dummy-key
RETELL_API_KEY=dummy-key
ELEVENLABS_API_KEY=dummy-key

# Other existing environment variables...
